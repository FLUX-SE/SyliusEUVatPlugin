{
    "name": "flux-se/sylius-eu-vat-plugin",
    "type": "sylius-plugin",
    "description": "Add EU VAT field and validation for Sylius using VIES API Webservice.",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis HILAIRE",
            "email": "Prometee@users.noreply.github.com"
        }
    ],
    "require": {
        "prometee/vies-client-bundle": "^3.0",
        "sylius/core-bundle": "^2.0"
    },
    "require-dev": {
        "ext-json": "*",
        "behat/behat": "^3.7",
        "dbrekelmans/bdi": "^1.2",
        "friends-of-behat/mink": "^1.9",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-debug-extension": "^2.0",
        "friends-of-behat/mink-extension": "^2.5",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.1",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/variadic-extension": "^1.3",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpstan/phpstan-webmozart-assert": "^1.0",
        "phpunit/phpunit": "^9.5",
        "robertfausk/behat-panther-extension": "^1.1",
        "sylius-labs/coding-standard": "^4.1",
        "sylius-labs/suite-tags-extension": "^0.2.0",
        "sylius/sylius": "^2.0",
        "symfony/browser-kit": "^6.4|^7.1",
        "symfony/debug-bundle": "^6.4|^7.1",
        "symfony/dotenv": "^6.4|^7.1",
        "symfony/intl": "^6.4|^7.1",
        "symfony/runtime": "^6.4|^7.1",
        "symfony/web-profiler-bundle": "^6.4|^7.1",
        "spaze/phpstan-stripe": "^3.2",
        "nyholm/psr7": "^1.8",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-symfony": "^1.4",
        "theofidry/alice-data-fixtures": "^1.7",
        "doctrine/orm": "^2.20",
        "doctrine/data-fixtures": "^1.8",
        "lchrusciel/api-test-case": "^5.3",
        "phpstan/phpstan-phpunit": "^1.4",
        "matthiasnoback/symfony-config-test": "^5.2"
    },
    "autoload": {
        "psr-4": { "FluxSE\\SyliusEUVatPlugin\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\FluxSE\\SyliusEUVatPlugin\\": "tests/",
            "Tests\\FluxSE\\SyliusEUVatPlugin\\App\\": "tests/Application/src/"
        }
    },
    "suggest": {
        "sylius/shop-bundle": "Use the Sylius default front shop",
        "sylius/admin-bundle": "Use the Sylius default admin",
        "sylius/api-bundle": "Use the Sylius default API"
    },
    "scripts": {
        "post-install-cmd": [
            "@php bin/create_node_symlink.php"
        ],
        "post-update-cmd": [
            "@php bin/create_node_symlink.php"
        ],
        "post-create-project-cmd": [
            "@php bin/create_node_symlink.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "phpstan/extension-installer": true,
            "symfony/flex": false,
            "symfony/runtime": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "runtime": {
            "project_dir": "tests/Application",
            "dotenv_path": "tests/Application/.env"
        }
    }
}
