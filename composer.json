{
    "name": "prometee/sylius-vies-client-plugin",
    "type": "sylius-plugin",
    "description": "VIES Client plugin for Sylius.",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "prometee/vies-client-bundle": "^1.0",
        "sylius/sylius": "^1.3"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink": "^1.7@dev",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^1.2",
        "friends-of-behat/cross-container-extension": "^1.1",
        "friends-of-behat/page-object-extension": "^0.3.0",
        "friends-of-behat/service-container-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^2.0.8",
        "friends-of-behat/variadic-extension": "^1.1",
        "lakion/mink-debug-extension": "^1.2.3",
        "phpspec/phpspec": "^5.0",
        "phpstan/phpstan-doctrine": "^0.10",
        "phpstan/phpstan-shim": "^0.10",
        "phpstan/phpstan-symfony": "^0.10",
        "phpstan/phpstan-webmozart-assert": "^0.10",
        "phpunit/phpunit": "^8.1",
        "sensiolabs/security-checker": "^5.0",
        "sylius-labs/coding-standard": "^3.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Prometee\\SyliusVIESClientPlugin\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Prometee\\SyliusVIESClientPlugin\\": "tests/",
            "Tests\\Prometee\\SyliusVIESClientPlugin\\App\\": "tests/Application/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "authors": [
        {
            "name": "Francis HILAIRE",
            "email": "prometee@gmail.com"
        }
    ]
}
