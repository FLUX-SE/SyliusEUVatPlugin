{
    "name": "prometee/sylius-vies-client-plugin",
    "type": "sylius-plugin",
    "description": "VIES Client plugin for Sylius.",
    "license": "MIT",
    "require": {
        "prometee/vies-client-bundle": "^1.0",
        "sylius/sylius": "^1.3"
    },
    "require-dev":  {
        "phpunit/phpunit": "^6.5"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Prometee\\SyliusVIESClientPlugin\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Prometee\\SyliusVIESClientPlugin\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "authors": [
        {
            "name": "Francis HILAIRE",
            "email": "prometee@gmail.com"
        }
    ]
}
